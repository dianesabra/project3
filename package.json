    "body-parser": "^1.19.0",
    "express": "^4.16.4",
    "if-env": "^1.0.4",
    "material-design-icons": "^3.0.1",
    "mongoose": "^5.0.17",
    "notistack": "^0.8.3",
    "react-dom": "^16.8.6",
    "react-images-upload": "^1.2.6",
    "react-material-ui-form-validator": "^2.0.8",
